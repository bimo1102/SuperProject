{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{ "Host": "api", "Port": 80 }],
      "UpstreamPathTemplate": "/api/{everything}",
      "SwaggerKey": "api"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "api",
      "Version": "v1",
      "Url": "http://api/swagger/v1/swagger.json"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:9000"
  }
}
